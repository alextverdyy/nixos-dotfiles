{
  "bar": {
    "launcher": {
      "autoDetectIcon": true
    },
    "workspaces": {
      "show_icons": true
    }
  },
  "layout": {
    "bar": {
      "layouts": {
        "0": {
          "left": [
            "dashboard",
            "workspaces"
          ],
          "middle": [
            "media"
          ],
          "right": [
            "volume",
            "systray",
            "notifications"
          ]
        }
      }
    }
  },
  "menus": {
    "clock": {
      "time": {
        "hideSeconds": true,
        "military": true
      },
      "weather": {
        "unit": "metric"
      }
    },
    "dashboard": {
      "directories": {
        "enabled": false
      },
      "stats": {
        "enable_gpu": true
      }
    }
  },
  "theme": {
    "bar": {
      "transparent": false
    },
    "font": {
      "name": "JetBrainsMono Nerd Font Regular",
      "size": "12px"
    }
  },
  "bar.clock.format": "%b %d  %H:%M",
  "bar.network.truncation": false
}
